/*Armazenar informações sobre os clientes criando suas tabelas e suas colunas*/

TABLE clientes(
  Clienteid INT PRIMARY KEY AUTOINCREMENT,
  Nome VARCHAR(100),
  Email VARCHAR(100),
  DataNacimento DATE,
  Cidade VARCHAR(50)
  );
  
  /*Inserindo dados na tabela cliente e testando pra ver se está funcionando*/
  
  INSERT INTO clientes(Nome, Email, DataNacimento, Cidade)
  VALUES ('Jadienne silva', 'jadiennesilva@gmail.com', '18-07-2003', 'Icó ceará');
          
  /*consultar dados*/
          
 SELECT * FROM clientes;
          
        
